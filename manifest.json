{
  "manifest_version": 2,

  "name": "Go3 Deeplinker",
  "description": "This extension will help create Go3 deeplinks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Find deeplink"
  },
  "permissions": [
    "activeTab"
  ]
}
